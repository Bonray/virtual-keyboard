{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", { "code": 300 }],
        "no-param-reassign": 0,
        "import/extensions": ["error", "ignorePackages", {
            "js": "always"
          }
        ]
    }
}
